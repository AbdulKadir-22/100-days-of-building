    "\n",
    "rng = np.random.default_rng()\n",
    "\n",
    "print(rng.integers(low = 1,high = 10,size = (3,3)))\n",
    "print()\n",
    "\n",
    "print(np.random.uniform(low = -1, high = 1))\n",
    "\n",
    "fruits = np.array([\"apple\", \"Banana\", \"coconut\", \"pineapple\", \"papaya\"])\n",
    "fruit = rng.choice(fruits)\n",
    "print()\n",
    "\n",
    "print(fruit)"
